{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".next",
  "installCommand": "npm ci --prefer-offline --no-audit --no-fund",
  "framework": "nextjs",
  "regions": ["fra1"],
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [],
  "github": {
    "silent": true
  }
}